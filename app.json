{
  "expo": {
    "name": "Navo",
    "slug": "Navo",
    "version": "1.9.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "navo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/magnifying glass and stars.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "imageResizeMode": "contain",
      "imageStyle": {
        "width": "200px",
        "height": "200px"
      }
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "requireFullScreen": true,
      "bundleIdentifier": "media.hilo.navo",
      "buildNumber": "54",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Navo needs access to your camera to take photos for your profile and posts.",
        "UIRequiresFullScreen": true
      },
      "associatedDomains": ["applinks:navo.go.link"],
      "usesAppleSignIn": true,
      "appleTeamId": "N8SACG5845"
    },

    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-apple-authentication",
      "@config-plugins/react-native-adjust",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "439d9c39-e83b-47b7-8094-65cff057c5e4"
      },
      "EXPO_PUBLIC_GEMINI_API_KEY": "AIzaSyCTChqF-pc0LSoXhKnDFC5gBxNcRvHzi9s",
      "router": {}
    }
  }
}
