{
  "expo": {
    "name": "Navo",
    "slug": "Navo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { 
      "supportsTablet": false,
      "bundleIdentifier": "media.hilo.navo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Navo needs access to your camera to take photos for your profile and posts."
      },
      "usesAppleSignIn": true
    },
    "android": {
      "versionCode": 7,
      "package": "media.hilo.navo"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e033667a-2828-432a-b429-71a8c2fd4bfe"
      },
      "EXPO_PUBLIC_GEMINI_API_KEY": "AIzaSyCTChqF-pc0LSoXhKnDFC5gBxNcRvHzi9s",
      "router": {}
    }
  }
}
